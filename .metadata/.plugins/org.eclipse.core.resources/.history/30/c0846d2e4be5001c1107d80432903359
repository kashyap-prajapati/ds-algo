package com.dc.merge.sort;

public class Main {
	
	static void merge(int arr,int l, int m, int r) {
		
	}
	
	static void mergeSort(int arr, int l, int r) {
		if(l<r) {
			int m = l + (r-l)/2;
			mergeSort(arr, l, m);
			mergeSort(arr, m+1, r);
			merge();
		}
	}
	public static void main(String[] args) {
		
		int arr[] = { 12, 11, 13, 5, 6, 7 };
		
	}

}
