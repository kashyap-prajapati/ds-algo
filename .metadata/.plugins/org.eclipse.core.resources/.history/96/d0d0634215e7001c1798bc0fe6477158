package com.algorithm.graph.bfs;

import java.util.LinkedList;

public class Main {

	static class Graph{
		int v;
		LinkedList<Integer> []adj;
		
		Graph(int v){
			this.v=v;
	        for (int i=0; i<v; ++i)
	            adj[i] = new LinkedList<>();
		}
		
		void addEdge(int v, int w){
			adj[v].add(w);
		}
	}
	
	public static void main(String []args) {
		
	}
}
